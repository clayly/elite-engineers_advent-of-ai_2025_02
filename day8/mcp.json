{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": [
        "mcp-server-git"
      ],
      "type": "stdio"
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "type": "stdio"
    },
    "weather": {
      "command": "uv",
      "args": [
        "run",
        "mcp.py"
      ],
      "type": "stdio"
    }
  }
}